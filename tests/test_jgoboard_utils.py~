_, _, _, left_col, upper_row, right_col, lower_row = process_pattern_for_jgoboard(
 """ 
= w .  
= . . 
= = = 
""")
assert (left_col, upper_row, right_col, lower_row) == ('A', 2, 'B', 1)

_, _, _, left_col, upper_row, right_col, lower_row = process_pattern_for_jgoboard(
""" 
=. . . . . . . 
=. . . . . b . 
=. . . . w b w 
=. . . w b w . 
=. . . b b w .  
=. . . . . . . 
= = = = = = = =
"""
)
assert (left_col, upper_row, right_col, lower_row) == ('A', 6, 'G', 1)

_, _, _, left_col, upper_row, right_col, lower_row = process_pattern_for_jgoboard(
